{% extends 'base.html.twig' %}

{% block title %}{{ 'Edit mission sensor'|trans }}{% endblock %}

{% block content %}
    <div class="container-fluid bg-white">
        <article class="mission-sensor py-3">
            <h1>{{ 'Edit mission sensor'|trans }}</h1>

            {{ form_start(form) }}
                {{ form_widget(form) }}

                <a href="{{ path_from_referer('mission_show', {id: mission_sensor.mission.id}) }}">{{ 'Cancel'|trans }}</a>
                <button class="btn btn-success">{{ mission_sensor.id ? 'Update'|trans : 'Create'|trans }}</button>
            {{ form_end(form) }}
        </article>
    </div>
{% endblock %}


{% block stylesheets %}
    {#{ parent() }#}
    {{ encore_entry_link_tags('mission_sensor/edit') }}
{% endblock %}

{% block javascripts %}
    {#{ parent() }#}
    {{ encore_entry_script_tags('mission_sensor/edit') }}
{% endblock %}
