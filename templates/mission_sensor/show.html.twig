{% extends 'base.html.twig' %}

{% block title %}{{ 'Mission sensor'|trans }}{% endblock %}

{% block body %}
    <h1>{{ 'Mission sensor'|trans }}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Name</th>
                <td>{{ mission_sensor.name }}</td>
            </tr>
            <tr>
                <th>Sensor</th>
                <td>{{ mission_sensor.sensor }}</td>
