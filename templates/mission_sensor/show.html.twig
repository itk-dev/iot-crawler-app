        </tbody>
    </table>

    <a href="{{ path('mission_sensor_index', {mission: mission_sensor.mission.id}) }}">back to list</a>

    <a href="{{ path('mission_sensor_edit', {mission: mission_sensor.mission.id, id: mission_sensor.id}) }}">edit</a>

    {{ include('mission_sensor/_delete_form.html.twig') }}
{% endblock %}
